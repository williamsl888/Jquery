  <script>
    $(document).ready(function(){
      $("button").click(function(){
        var txt="";
        txt+="Width of div: "+$("#div1").width()+"<br>";
        txt+="Height of div: "+$("#div1").height();
        $("#div1").html(txt);
      });
    });
    </script>
<div id="div1"></div>
