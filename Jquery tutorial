        $(document).ready(function(){
          $("button").click(function(){
            $("h1,h2,p").addClass("blue");
            $("div").addClass("important");
          })
        })
        </script>
        <style>
          .important{
            font-weight:bold;
            font-size:xx-large;
          }
          .blue{
            color:blue;
          }
          </style>
    </head>
