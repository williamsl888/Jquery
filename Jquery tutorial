    <script>
      $(document).ready(function(){
        $("#btn1").click(function(){
          $("#test1").text(function(i,origText){
            return "Old text: "+origText+" New text: This is new paragraph-1";
          });
        });
        $("#btn2").click(function(){
          $("#test2").html(function(i,origText){
            return "Old text: "+origText+ " New html: This is new paragraph-2";
          });
        });
      });
      </script>
  </head>
