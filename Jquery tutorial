<script>
  $(document).ready(function(){
    function animateDiv()
    {
      $("div:eq(0)").animate({width:"50%"},"slow");
      $("div:eq(0)").animate({width:"100%"},"slow",animateDiv);
    }
    animateDiv();
    $(".btn1").click(function(){
      $(":animated").css("background-color","red");
    });
  });
  </script>
<button class="btn1">Change color of the animated element</button>
